{
   "redirects": [
    {
      "source": "/",
      "destination": "/appinsurance",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/appinsurance/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/appinsurance/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/appinsurance",
      "destination": "/index.html"
    }
  ]
}